<h1>Events List</h1>
<table class="table table-bordered">
	<% @events.each do |e| %>
		<tr>
			<td>
				<%= e.name %>
			</td>
			<td>
			<%= link_to 'Show', event_path(e), :class => "btn btn-primary" %>
			
			<%= link_to 'Go Json', event_path(e,:format => :json) , :class => "btn btn-default"%>
			
			<%= link_to 'Edit', edit_event_path(e), :class => "btn btn-default" %>
			
			<%= link_to 'Delete', event_path(e), :method => :delete , :class => "btn btn-danger"%>
			</td>
		</tr>
	<% end %>
</table>
<%= link_to 'New', new_event_path,:class => "btn btn-primary"%>
<br/>
<%= paginate @events %>